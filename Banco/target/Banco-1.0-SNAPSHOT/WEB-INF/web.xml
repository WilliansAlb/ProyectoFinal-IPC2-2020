    </servlet>
    <servlet>
        <servlet-name>modificacion</servlet-name>
        <servlet-class>Servlet.modificacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>transaccion</servlet-name>
        <servlet-class>Servlet.transaccion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>configuracion</servlet-name>
        <servlet-class>Servlet.configuracion</servlet-class>
