        <servlet-name>modificacion</servlet-name>
        <url-pattern>/modificacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>transaccion</servlet-name>
        <url-pattern>/transaccion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>configuracion</servlet-name>
        <url-pattern>/configuracion</url-pattern>
    </servlet-mapping>
